{
  "buildCommand": "echo 'Static site - no build needed'",
  "outputDirectory": ".",
  "public": true,
  "routes": [
    {
      "src": "/(.*\\.html)$",
      "dest": "/$1"
    },
    {
      "src": "/(.*\\.json)$",
      "dest": "/$1"
    },
    {
      "src": "/(.*\\.svg)$",
      "dest": "/$1"
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ]
}

